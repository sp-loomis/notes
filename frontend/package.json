{
  "name": "@notes/frontend",
  "version": "0.1.0",
  "description": "Electron frontend for the notes app",
  "main": "./dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:renderer": "cross-env NODE_ENV=development webpack serve --config webpack.renderer.dev.js",
    "dev:main": "wait-on http://localhost:3000 && npx tsc --outDir dist/main src/main/*.ts --esModuleInterop --skipLibCheck && cross-env NODE_ENV=development electron .",
    "build": "concurrently \"npm run build:main\" \"npm run build:preload\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "build:preload": "cross-env NODE_ENV=production webpack --config webpack.preload.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.js",
    "start": "electron .",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@notes/database": "file:../database",
    "@reduxjs/toolkit": "^2.1.0",
    "electron-store": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wait-on": "^8.0.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
